                                <FontIcon x:Name="CheckGlyph" Foreground="{ThemeResource CheckBoxCheckGlyphForegroundUnchecked}" FontFamily="{ThemeResource SymbolThemeFontFamily}" FontSize="20" Glyph="&#xE001;" Opacity="0" RenderTransformOrigin="0.5,0.5" Width="15" FocusVisualPrimaryBrush="#E4D0D0D0">
                                    <FontIcon.RenderTransform>
